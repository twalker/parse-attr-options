  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "chai": "^3.5.0",
    "mocha": "^2.1.0",
    "watch": "^0.18.0"
  }
